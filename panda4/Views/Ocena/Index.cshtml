@model IEnumerable<panda4.Models.OcenaModel>

@{
    ViewData["Title"] = "Index";
}

<h2>Ocen komputer!</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.KomputerID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ocena)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KomputerID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ocena)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.OcenaID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.OcenaID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.OcenaID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
